{
    "manifest_version": 3,
    "name": "Shopee Product Uploader",
    "description": "快速填寫商品資訊並送出至 Shopee 後台 API。",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["storage"],
    "host_permissions": [
      "http://localhost:8000/*"
    ]
  }
  